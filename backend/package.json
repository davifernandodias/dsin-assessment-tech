{
  "name": "backend",
  "version": "1.0.0",
  "description": "backend for compass",
  "main": "dist/index.js",
  "scripts": {
    "start": " node dist/index.js",
    "dev": "tsx --watch src/index.ts",
    "build": "tsup src/index.ts --format cjs,esm --dts --minify",
    "migration:generate": "npx drizzle-kit generate",
    "push": "npx drizzle-kit push",
    "studio": "npx drizzle-kit studio --verbose",
    "test": "jest"
  },
  "keywords": [
    "express",
    "typescript",
    "postgres",
    "drizzle",
    "api"
  ],
  "author": "Davi Dias",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/pg": "^8.11.11",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.40.0",
    "drizzle-zod": "^0.7.0",
    "express": "^4.21.2",
    "pg": "^8.14.0",
    "postgres": "^3.4.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.6.0",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "drizzle-kit": "^0.19.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  }
}
